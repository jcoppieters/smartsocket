{
  "version": "0.0.1",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Test Platform",
      "program": "${workspaceFolder}/index.spec.js",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}