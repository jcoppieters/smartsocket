<script type="text/javascript" src="/files/min.js"></script>

<script type="text/javascript">
  document.addEventListener('DOMContentLoaded', function() { 
    Array.prototype.forEach.call(document.getElementsByClassName("indeterminate"), element => {
      element.indeterminate = true;
    });
    M.AutoInit();
    const msg = `<%= (typeof message === 'undefined') ? '' : message %>`;
    if (msg) M.toast({html: msg});
    console.log("Ready to rock & roll");
  }, false);

  document.addEventListener("keyup", function(e) {
    if (e.key == "Enter") {
      e.preventDefault();
    }
  });

  function ajax(url, success) {
    var xhr = window.XMLHttpRequest ? new XMLHttpRequest() : new ActiveXObject('Microsoft.XMLHTTP');
    xhr.open('GET', url);
    xhr.onreadystatechange = function() {
        if (xhr.readyState>3 && xhr.status==200) success(xhr.responseText);
    };
    xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
    xhr.send();
    return xhr;
  }
</script>